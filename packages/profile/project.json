{
  "name": "profile",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "packages/profile/src",
  "projectType": "library",
  "tags": ["react-core", "profile"],
  "targets": {
    "lint": {
      "executor": "@nx/eslint:lint",
      "outputs": ["{options.outputFile}"],
      "options": {
        "lintFilePatterns": ["packages/profile/**/*.{ts,tsx,js,jsx}"]
      }
    },
    "build": {
      "executor": "@nx/rollup:rollup",
      "outputs": ["{options.outputPath}"],
      "options": {
        "outputPath": "dist/packages/profile",
        "tsConfig": "packages/profile/tsconfig.lib.json",
        "project": "packages/profile/package.json",
        "entryFile": "packages/profile/src/index.ts",
        "external": ["react", "react-dom", "react/jsx-runtime"],
        "rollupConfig": "@nx/react/plugins/bundle-rollup",
        "compiler": "babel",
        "assets": [
          {
            "glob": "packages/profile/README.md",
            "input": ".",
            "output": "."
          }
        ]
      }
    },
    "test": {
      "executor": "@nx/jest:jest",
      "outputs": ["{workspaceRoot}/coverage/{projectRoot}"],
      "options": {
        "jestConfig": "packages/profile/jest.config.ts"
      }
    }
  }
}
